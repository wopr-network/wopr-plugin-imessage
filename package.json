{
  "author": "TSavo",
  "dependencies": {
    "@wopr-network/plugin-types": "^0.1.0",
    "winston": "^3.11.0"
  },
  "description": "iMessage/SMS integration for WOPR via imsg CLI (macOS only)",
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/node": "^20.0.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "wopr",
    "plugin",
    "imessage",
    "sms",
    "macos"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-imessage",
  "os": [
    "darwin",
    "linux"
  ],
  "peerDependencies": {
    "wopr": ">=0.0.1"
  },
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "dev": "tsx src/index.ts",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "test": "vitest run",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
